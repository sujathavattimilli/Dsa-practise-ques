static int calculator(char ch, int a, int b)
    {
         int i = 0;
         switch(ch) {
           case '/':
              i = a/b;
             break;
           case '*':
             i = a*b;
             break;
           case '-':
             i = a-b;
             break;
           case '+':
             i = a+b;
              break;
         }
         return(i);
     }
